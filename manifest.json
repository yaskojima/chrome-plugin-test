{
    "manifest_version": 2,
    "name": "TabpagesTransparentTool",
    "version": "0.0.0.1",
    "browser_action": {
        "default_popup": "tttmenu.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["http://*.jp/*"],
            "js": ["jquery-3.3.1.min.js", "fuga.js"],
            "run_at": "document_idle"
        }
    ]
}
